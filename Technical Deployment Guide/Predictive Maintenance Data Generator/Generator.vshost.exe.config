<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
  <runtime>
    <loadFromRemoteSources enabled="true" />
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="Newtonsoft.Json" publicKeyToken="30ad4fe6b2a6aeed" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-7.0.0.0" newVersion="7.0.0.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
  <appSettings>
    <!-- Service Bus specific app setings for messaging connections -->
    <add key="EventHubName" value="[IngestEventHubName]" />
    <add key="EventHubConnectionString" value="[EventHubConnectionString]" />
    <add key="StorageAccountConnectionString" value="[StorageAccountConnectionString]" />
    <add key="SqlServerName" value="[SqlServerName]" />
    <add key="SqlDatabaseName" value="[SqlServerDatabaseName]" />
    <add key="User" value="[SqlUser]" />
    <add key="Credential" value="[SqlPass]" />
  </appSettings>
</configuration>